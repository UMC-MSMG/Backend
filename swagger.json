{
  "openapi": "3.0.0",
  "info": {
    "title": "MSMG",
    "version": "1.0.0",
    "description": "MSMG node.js swagger"
  },
  "paths": {
    "/": {
      "get": {
        "summary": "Returns a greeting message",
        "responses": {
          "200": {
            "description": "A successful response",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string",
                  "example": "Hello World!"
                }
              }
            }
          }
        }
      }
    },
    "/api/points/{userId}": {
      "get": {
        "summary": "Get user points",
        "parameters": [
          {
            "in": "path",
            "name": "userId",
            "required": true,
            "schema": {
              "type": "integer"
            },
            "description": "The user ID"
          }
        ],
        "responses": {
          "200": {
            "description": "A successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "userId": {
                      "type": "integer"
                    },
                    "point": {
                      "type": "integer"
                    },
                    "message": {
                      "type": "string",
                      "example": "포인트 조회성공"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
